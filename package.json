{
  "name": "string.ify",
  "version": "1.0.34",
  "description": "A cross-platform JavaScript object stringifier / pretty-printer",
  "main": "string.ify.js",
  "scripts": {
    "lint": "eslint string.ify.js",
    "lint-test": "eslint test.js",
    "build": "npm run lint && npm run lint-test && npm run babel",
    "test": "env STRING_IFY_TEST_FILE='./string.ify' nyc --reporter=html --reporter=text mocha --reporter spec",
    "test-es5": "env STRING_IFY_TEST_FILE='./build/string.ify.es5' mocha --reporter spec",
    "autotest": "env STRING_IFY_TEST_FILE='./string.ify' mocha --reporter spec --watch",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "babel": "babel string.ify.js --source-maps inline --out-file ./build/string.ify.es5.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpl/string.ify.git"
  },
  "keywords": [
    "stringify",
    "toString",
    "pretty-print",
    "object",
    "string",
    "object printer"
  ],
  "author": "Vitaly Gordon <rocket.mind@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/xpl/string.ify/issues"
  },
  "homepage": "https://github.com/xpl/string.ify",
  "devDependencies": {
    "browserify": "^14.4.0",
    "mocha": "^3.4.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "istanbul": "^0.4.5",
    "nyc": "^11.1.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2017-object-entries": "0.0.4",
    "babel-preset-es2015": "^6.24.1"
  },
  "dependencies": {
    "string.bullet": "^1.0.9"
  }
}
